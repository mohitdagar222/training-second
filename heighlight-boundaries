let map;

function initMap() {

    var data = {
        type: "Feature",
        geometry: {
            "type": "Polygon",
            "coordinates": [
                [
                    [-73.974228, 40.75597],
                    [-73.983841, 40.742931],
                    [-74.008133, 40.75307500000001],
                    [-73.998131, 40.765915],
                    [-73.974228, 40.75597]
                ]
            ]
        }
    };
    var map = new google.maps.Map(document.getElementById('map-canvas'), {
        zoom: 5,
        center: { lat: 40.75597, lng: 0.1276 }
    });
    map.data.addGeoJson(data);



}





