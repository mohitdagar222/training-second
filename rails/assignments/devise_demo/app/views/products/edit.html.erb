<h3><%= link_to "List Products", "/products" %> | <%= link_to "Logout", "/logout" %></h3>
<%= form_for @product do |f| %>
    <div id = "form2">
    <h2>Edit Product </h2>
   <%= f.label :name, "Enter Product Name", class: :label %>
    <%= f.text_field :name %><br><br>
